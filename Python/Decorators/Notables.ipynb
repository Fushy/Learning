   "source": [
    "def auto_repr(cls):\n",
    "    def __repr__(self):\n",
    "        return '%s(%s)' % (type(self).__name__, ', '.join('%s=%s' % item for item in vars(self).items()))\n",
    "\n",
    "    cls.__repr__ = __repr__\n",
    "    return cls"
   ],
